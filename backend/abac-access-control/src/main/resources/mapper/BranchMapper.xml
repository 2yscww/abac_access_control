<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xie.platform.mapper.BranchMapper">

    <!-- 根据分公司ID查询 -->
    <select id="selectById" resultType="com.xie.platform.model.Branch">
        SELECT
            branch_id,
            branch_name
        FROM branches
        WHERE branch_id = #{branchId}
    </select>

</mapper>
